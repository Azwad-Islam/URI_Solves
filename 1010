#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int a1{}, a2{}, quantity1{}, quantity2{};
        float price1{}, price2{}, vtp{};
    cin >> a1 >> quantity1 >> price1;
    cin >> a2 >> quantity2 >> price2;

    vtp = ((quantity1 * price1) + (quantity2 * price2));

        cout << "Valor a pagar: R$ " << vtp;


    return 0;
}
